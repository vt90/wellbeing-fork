{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app"
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts"
  ],
  "exclude": [
    "src/**/*.spec.ts",
    "src/pages/**/*.ts",
    "src/providers/*",
    "src/validators/*",
    "src/app/*.original.ts",
    "src/environments/*.template.ts"
  ]
}
